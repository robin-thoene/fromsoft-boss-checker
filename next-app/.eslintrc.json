{
  "extends": [
    "next/core-web-vitals",
    "plugin:jsdoc/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "jsdoc",
    "simple-import-sort",
    "import"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "import/first": "error",
    "import/newline-after-import": "error"
  }
}
